class Solution:
    def goodNodes(self, root: TreeNode) -> int:
        def dfs(node,mx):
            if node.val>=mx:
                self.cnt+=1
            if node.left:
                dfs(node.left,max(mx,node.val))
            if node.right:
                dfs(node.right,max(mx,node.val))
        self.cnt=0
        dfs(root,root.val)
        return self.cnt
